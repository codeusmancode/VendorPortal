<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="challan_summary">
    <default-activity>ExecuteWithParams</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <input-parameter-definition id="__3">
      <name>challan_id</name>
      <value>#{pageFlowScope.challan_id}</value>
      <class>java.lang.Integer</class>
    </input-parameter-definition>
    <input-parameter-definition id="__6">
      <name>challan_number</name>
      <value>#{pageFlowScope.challan_number}</value>
      <class>java.lang.String</class>
    </input-parameter-definition>
    <view id="summary">
      <page>/fragments/summary.jsff</page>
    </view>
    <task-flow-return id="taskFlowReturn1">
      <outcome>
        <name>taskFlowReturn1</name>
      </outcome>
    </task-flow-return>
    <method-call id="ExecuteWithParams">
      <method>#{bindings.ExecuteWithParams.execute}</method>
      <outcome>
        <fixed-outcome>ExecuteWithParams</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__1">
      <from-activity-id>summary</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>return</from-outcome>
        <to-activity-id>taskFlowReturn1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>ExecuteWithParams</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>ExecuteWithParams</from-outcome>
        <to-activity-id>summary</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
